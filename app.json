{
  "expo": {
    "name": "expo-react-native-app",
    "slug": "expo-react-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "BakeBot needs photo access to help with your cooking questions and share images of your recipes.",
        "NSCameraUsageDescription": "BakeBot needs camera access to see your cooking and help with recipe questions.",
        "NSMicrophoneUsageDescription": "BakeBot needs microphone access to hear your cooking questions and provide voice assistance."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sjufan84.exporeactnativeapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "30595a2f-c75b-4e4a-b2ed-61e19e39c482"
      }
    }
  }
}
